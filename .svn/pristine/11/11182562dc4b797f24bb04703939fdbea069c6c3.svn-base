<div id="visit" class="w_h_500" style="display:none;">
    <div class="m_t6 m_l_24">
        <div class="text_r">
            <i class="layui-icon layui-icon-close" style="font-size: 22px;" id="close">&#x1006;</i>
        </div>
        <!-- 星星 -->
        <div class="eval-star">
            <span class="eval-star_o l_h_30">评分：</span>
